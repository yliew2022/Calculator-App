                    int count = 0;
                    float c = 0;
                    for (c = 0; c < 50; c++)
                    {
                        cout << "Enter the values: ";
                        cin >> values[c];
                    }
                    for (c = 0; c < 50; c++)
                    {
                        if (values[c] != '\0')
                            count++;
                    }